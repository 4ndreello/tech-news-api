# Server Port (optional, default: 8080)
PORT=8080

# Valkey (Memorystore) Configuration
# GCP Memorystore (Valkey) connection details
VALKEY_HOST=10.x.x.x
VALKEY_PORT=6379
VALKEY_PASSWORD=your_valkey_password_here
VALKEY_DB=0

# Twitter/X API Credentials
# Prefer TWITTER_BEARER_TOKEN for better rate limits
TWITTER_BEARER_TOKEN=your_bearer_token_here
TWITTER_API_KEY=your_api_key_here
TWITTER_API_SECRET=your_api_secret_here
